<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Management System</title>
    <link rel="stylesheet" href="./css/main.css">
</head>
<body>
    <%- include('./partials/nav') %>
    <div class="container">
        <div class="add-dish">
            <a href="/add-dish" type="button" class="btn add-dish__btn">Add Dish</a>
        </div>
        <% data.forEach(tuple => { %>
            <div class="data-point">
                <div class="data-point__container">
                    <p><%= tuple.DishName %></p>                    
                    <div class="actions">
                        <a class="btn" href="#"><img src="./images/mode_edit_white_24dp.svg" alt=""></a>
                        <a class="btn" href="#"><img src="./images/delete_white_24dp.svg" alt=""></a>
                    </div>
                </div>
            </div>
            <% }); %>
    </div>
</body>
</html>